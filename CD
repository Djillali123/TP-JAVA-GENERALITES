public class CD extends Media {
    private String artiste;
    private int duree;

    public CD(String artiste, int anneePublication, String title, int duree) {
        super(title, anneePublication);
        this.artiste = artiste;
        this.duree = duree;
    }

    @Override
    public String getDescription() {
        return "CD de" + artiste + ", dur√©e :" + duree + "min.";
    }
}
